          function blurtablebuttons(clickedButton,tablename){
            var table = clickedButton.parentNode.parentNode.parentNode.parentNode;
            var buttons = table.getElementsByTagName("button");
            for (var i = 0; i < buttons.length; i++){
              if (buttons[i]==clickedButton)
              {
                continue;
              }
              buttons[i].style.filter = "blur(3px)";
            }
          }
